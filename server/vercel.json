{
    "version":2,
    "builds": [
        { "scr": "*.ts", "use": "@vercel/node"}
    ],
    "rewrites": [{ "source": "/(.*)", "destination": "/index.html" }],
    "routes": [
        {
            "scr": "/(.*)",
            "dest": "/",
            "methods": ["GET", "POST", "PUT", "DELETE", "PATCH", "OPTIONS"],
            "headers": {
                        "Access-Control-Allow-Origin": "*"
            }
        }
    ]
}
